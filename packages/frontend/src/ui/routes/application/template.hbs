<TopNav id='emberclear-topnav'/>

<NotificationContainer @position='top-right' />
<NotificationPrompt />

<OffCanvasContainer
  class='no-overflow'
  @contacts={{model.contacts}}
  @channels={{model.channels}}
>
  <ServiceWorkerUpdateNotify @pollingInterval={{60000}}>
    <a class='service-worker-update-notify notification is-info' href='' style='z-index: 100;'>
      {{t 'status.updateAvailable'}}
    </a>
  </ServiceWorkerUpdateNotify>

  {{outlet}}

</OffCanvasContainer>

<Modals />
<AppShellRemover />
